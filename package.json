    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.3.1",
    "ts-alias": "^0.0.7",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
